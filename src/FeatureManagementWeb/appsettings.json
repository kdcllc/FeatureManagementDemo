{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  // configuration for the feature management
  "FeatureManagement": {
    "Beta": true, // don't forget to create enum or const for this value
    "Alpha": false,

    "BrowserRenderer": {
      "EnabledFor": [
        {
          "Name": "Browser",
          "Parameters": {
            // "AllowedBrowsers": [ "Chrome", "Edge" ]
            "AllowedBrowsers": [ "Chrome" ]
          }
        }
      ]
    }
  }
}
