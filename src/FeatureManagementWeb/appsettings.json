{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "AppConfig": {
    "Endpoint": "https://featuremanagementworkshop.azconfig.io",
    "RefreshInterval": "00:00:15"
  },

  // configuration for the feature management
  "FeatureManagement": {
    "Beta": true, // don't forget to create enum or const for this value
    "Alpha": false,

    "Cached": {
      "EnabledFor": [
        {
          "Name": "Microsoft.Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },

    "BrowserRenderer": {
      "EnabledFor": [
        {
          "Name": "Browser",
          "Parameters": {
            // "AllowedBrowsers": [ "Chrome", "Edge" ]
            "AllowedBrowsers": [ "Chrome" ]
          }
        }
      ]
    }
  }
}
