{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-worker",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/FeatureManagementWorkerDemo/FeatureManagementWorkerDemo.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish-woker",
            "command": "dotnet",
            "type": "process",
            "args": [
